{
  "name": "walax",
  "version": "0.1.2",
  "description": "walax is a Javascript framework for interacting with objects remotely via Django.",
  "main": "dist/walax.js",
  "scripts": {
    "test": "jasmine",
    "start": "webpack -d --watch",
    "build": "webpack -d --watch",
    "docs": "documentation build src/** -f html -o docs/"
  },
  "keywords": [
    "walax",
    "javascript",
    "django"
  ],
  "repository": "https://github.com/hazelmollusk/walax",
  "author": "Matt Barry <matt@hazelmollusk.org>",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "sourceMaps": true,
    "plugins": [
      [
        "@babel/plugin-transform-runtime"
      ],
      [
        "@babel/plugin-syntax-dynamic-import"
      ],
      [
        "@babel/plugin-proposal-optional-chaining"
      ],
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-proposal-logical-assignment-operators"
      ]
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/parser": "^7.12.5",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@babel/types": "^7.12.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "chokidar": "^3.4.3",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.5",
    "documentation": "^13.1.0",
    "mithril": "^2.0.4",
    "mobx": "^5.15.7",
    "prettier": "^2.1.2",
    "stackinfo": "^1.1.5",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@babel/code-frame": "^7.10.4",
    "@babel/preset-react": "^7.12.5",
    "@babel/template": "^7.10.4",
    "babel-plugin-macros": "^2.8.0",
    "babel-preset-react-app": "^10.0.0",
    "jasmine": "^3.6.3"
  }
}
