{
  "name": "walax",
  "version": "0.0.5",
  "description": "walax is a Javascript framework for interacting with objects remotely via Django.",
  "main": "dist/walax/index.js",
  "scripts": {
    "start": "webpack -d --watch"
  },
  "keywords": [
    "walax",
    "javascript",
    "django"
  ],
  "repository": "https://github.com/hazelmollusk/walax",
  "author": "Matt Barry <matt@hazelmollusk.org>",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "sourceMaps": true,
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      "@babel/plugin-proposal-logical-assignment-operators"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/parser": "^7.11.3",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.11.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@babel/types": "^7.11.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "chokidar": "^3.4.2",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.5",
    "mithril": "^2.0.4",
    "mobx": "^5.15.5",
    "stackinfo": "^1.1.5",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "prettier": "^2.0.5"
  }
}
