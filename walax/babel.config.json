{
  "presets": [ [
    "@babel/preset-env",
    {
      "targets": {
        "node": 10
      }
    } ] ],
  "sourceMaps": true,
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    "@babel/plugin-proposal-logical-assignment-operators"
   ]
}
